for _ in range(int(input())):
    c,d,l=map(int,input().split())
    tl=(c*4)+(d*4)
    if (l%4==0) and (d*4<=l<=tl):
        if (c>d):
            if(d+(c-d*2))*4<=l:
                print("yes")
            else:
                print("no")
        else:
            print("yes")
    else:
        print("no")
